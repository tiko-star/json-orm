{
    "name": "tiko/json-orm",
    "type": "project",
    "require": {
        "slim/psr7": "^1.2",
        "slim/slim": "4.*",
        "symfony/property-access": "^5.1",
      "ext-json": "*",
        "php-di/php-di": "^6.2",
        "doctrine/orm": "^2.7"
    },
    "authors": [
        {
            "name": "Tigran Mesropyan",
            "email": "tikodeveloper@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "symfony/var-dumper": "^5.1",
        "phpstan/phpstan": "^0.12.42"
    },
    "scripts": {
        "analyse": [
            "vendor/bin/phpstan analyse -c phpstan.neon --ansi"
        ]
    }
}
